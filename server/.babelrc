{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", { "regenerator": true }],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@components": "../src/components",
        "@store": "../src/store",
        "@styles": "../src/styles",
        "@hooks": "../src/hooks",
        "@utils": "../src/utils"
      }
    }]
  ]
} 