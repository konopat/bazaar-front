    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.5.3",
    "jsdom": "^26.0.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.2",
