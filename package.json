{
  "name": "bazaar-front",
  "version": "1.0.0",
  "description": "Flower bouquet online store",
  "homepage": "https://konopat.github.io/bazaar-front",
  "main": "index.js",
  "scripts": {
    "start:dev:client": "webpack serve --mode development --open",
    "start:dev:server": "cd server && nest start --watch",
    "start:dev": "npm-run-all --parallel start:dev:client start:dev:server",
    "build:client": "NODE_ENV=production webpack --mode production",
    "build:server": "cd server && nest build",
    "build": "npm-run-all build:client build:server",
    "start": "cd server && npm run start:prod",
    "analyze": "ANALYZE=true webpack --mode production",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "predeploy": "NODE_ENV=production webpack --mode production",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.7",
    "@nestjs/core": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/serve-static": "^4.0.1",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/express": "^4.17.21",
    "@types/ol": "^6.5.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "localforage": "^1.10.0",
    "ol": "^10.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@babel/register": "^7.23.7",
    "@babel/runtime": "^7.24.0",
    "@nestjs/cli": "^10.3.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.8.1",
